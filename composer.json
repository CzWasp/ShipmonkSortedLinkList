{
  "require": {
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "php": ">=8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "slevomat/coding-standard": "~8.0",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Shipmonk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shipmonk\\": "tests/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit tests"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
